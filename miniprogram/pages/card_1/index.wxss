.header{
  height:100rpx;
  display: flex;
  padding-left:20rpx;
  padding-right:20rpx;
  align-items: center;
  flex-direction: row;
  background: #fff;
}

.search{
  width:100%;
  height:70rpx;
  background: rgb(245, 245, 245);
  border-radius:30rpx;
  padding-left: 20rpx;
  display: flex;
  align-items: center;
}

input{
  font-size: 30rpx;
  color: #888;
  flex:1;
  margin-left: 20rpx;
}

.all{
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  margin: 0rpx 20rpx;

}
